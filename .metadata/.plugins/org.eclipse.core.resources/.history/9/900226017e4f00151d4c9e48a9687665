#include <iostream>
#include <vector>
using namespace std;

void BubbleSort(vector<int> &v)
{
	int vector_size = v.size();
	bool swapped = false;
	do
	{
		for(int i = 0; i < vector_size - 1; i++)
		{
			if(v[i] > v[i] + 1)
				swap(v[i], v[i + 1]);
			swapped = true;
		}
	} while(!swapped);
}

int main()
{
	return 0;
}




