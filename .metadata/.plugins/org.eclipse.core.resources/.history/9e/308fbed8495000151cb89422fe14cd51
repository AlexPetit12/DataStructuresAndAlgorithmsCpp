#include <algorithm>
#include <iostream>
#include <vector>
using namespace std;

template <class T>
void InsertionSort(vector<T> &v)
{
	for(int i = 1; i < v.size(); i++)
	{
		for(int j = i; j > 0; j--)
		{
			if(v[j + 1] < v[j])
			{
				swap(v[j + 1], v[j]);
			}
		}
	}
}

int main()
{
	//test vector of ints
	vector<int> v_ints = {5,1,3,7,9,11,2,2,0,8};
	InsertionSort(v_ints);
	for(int &e : v_ints)
		cout << e << " ";

	return 0;
}




